{
    "version": 2,
    "builds": [
      {
        "src": "./SERVER/server.js", 
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/build",   
        "use": "@vercel/static"
      }
    ],
  

    "rewrites": [
        {
          "source": "/api/(.*)",
          "destination": "/SERVER/server.js"
        },
        {
          "source": "/(.*)",
          "destination": "/frontend/build/index.html"
        }
      ]
      
  }